    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "husky": "^1.3.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
